#include <stdio.h>
#include <stdlib.h>
#include <stdlib.h>


int main(void) {

    FILE *file;
    file = fopen("teste.txt", "r");
    if (!file) {
        printf("Não foi possível abrir o arquivo!\n");
        return 0;
    }


    char palavra[100];

    while (fgets(palavra, 100, file) != NULL)
    {
        printf("%s", palavra);
    }
    


    // while (fscanf(file, " %[^\n]s", palavra) == 1)
    // {
    //     printf("%s\n", palavra);
    // }
    

    printf("\n\n fim do arquivo.");
    fclose(file);


}
